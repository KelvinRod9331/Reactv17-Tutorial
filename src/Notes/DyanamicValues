function App() {
  const title = "Welcome to the new blog";
  const likes = ["50", 51];
  const person = { name: "kelvin", age: 29 };
  //You can output numbers, strings and array but cannot output objects and booleans

  //Curly braces are meant to output values
  return (
    <div className="App">
      <div className="content">
        <h1>{title}</h1>
        <p>Liked {likes} times</p>
      </div>
    </div>
  );
}

export default App;